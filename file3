X=[]
Y=[]
i=0
while(i<k): 
    X.append(input('enter x-'))
    Y.append(input('enter y-'))
    for j in range (0,k):
        prod=1
        if i!=j:
            prod=prod*(x-X[j])*1/(X[i]-X[j])
    prod=prod*Y[i]
    retval=retval+prod
    i=i+1
print('the value at x is -',retval)
